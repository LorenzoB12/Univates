	<queryString>
		<![CDATA[SELECT C.ID,
       C.DATA,
	   F.NOME,
	   IT_CO.QTDE,
	   (IT_CO.VALOR * IT_CO.QTDE) AS VALOR_TOTAL,
	   P.DESCRICAO
  FROM COMPRAS C
      ,ITENS_COMPRAS IT_CO
	  ,PRODUTOS P
	  ,FORNECEDORES F
 WHERE C.ID = IT_CO.COMPRA_ID
   AND P.ID = IT_CO.PRODUTO_ID
   AND F.ID = C.ID_FORNECEDOR
   AND F.NOME LIKE '$P!{fornecedor}']]>
